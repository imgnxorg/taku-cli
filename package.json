{
  "name": "gittaku",
  "version": "2.6.299",
  "description": "Taku CLI: Project generator and utility for the Taku framework.",
  "bin": {
    "taku": "./dist/src/cli.js"
  },
  "scripts": {
    "build": "esbuild src/cli.js --bundle --platform=node --outfile=dist/src/cli.js --format=esm --packages=external && esbuild src/taku.config.parse.js --bundle --platform=node --outfile=dist/src/taku.config.parse.js --format=esm --packages=external",
    "prepublishOnly": "yarn build",
    "prepare": "yarn build",
    "start": "node dist/cli.js"
  },
  "type": "module",
  "author": "Donald Moore @imgnx <donaldwaynemoorejr@gmail.com>",
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/imgnxorg/taku-cli.git"
  },
  "keywords": [
    "cli",
    "taku",
    "framework",
    "project-generator"
  ],
  "packageManager": "yarn@4.9.2",
  "dependencies": {
    "chalk": "^5.4.1",
    "fs-extra": "^11.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "pkg": {
    "assets": [
      "main.sh",
      "config.json"
    ],
    "external": [
      "fs",
      "fs-extra",
      "child_process"
    ],
    "compile": false,
    "outputPath": "./bin"
  }
}
